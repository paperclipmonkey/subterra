{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "#/objects/user",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "minimum": 1
    },
    "name": {
      "type": "string",
      "minLength": 1
    },
    "photo": {
      "type": ["string", "null"]
    },
    "clubs": {
      "type": "array",
      "items": {
        "$ref": "#/objects/club-summary"
      }
    }
  },
  "required": ["id", "name", "clubs"],
  "additionalProperties": false
}