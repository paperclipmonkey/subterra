{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "#/objects/club",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "minimum": 1
    },
    "name": {
      "type": "string",
      "minLength": 1
    },
    "slug": {
      "type": "string",
      "minLength": 1
    },
    "description": {
      "type": ["string", "null"]
    },
    "is_active": {
      "type": "boolean"
    }
  },
  "required": ["id", "name", "slug", "is_active"],
  "additionalProperties": false
}